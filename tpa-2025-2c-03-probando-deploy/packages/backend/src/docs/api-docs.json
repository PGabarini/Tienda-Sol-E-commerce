        "/vendedor/:id/productos": {
            "get": {
                "tags": [
                    "Productos"
                ],
                "parameters": [
                    {
                        "name": "id",
                        "description": "Identificador único del vendedor.",
                        "schema": {
                            "type": "string"
                        },
                        "in": "path",
                        "required": true
                    },
                    {
                        "name": "nombre",
                        "description": "Filtra los productos por nombre.",
                        "schema": {
                            "type": "string"
                        },
                        "in": "query"
                    },
                    {
                        "name": "descripcion",
                        "description": "Filtra los productos por descripción.",
                        "schema": {
                            "type": "string"
                        },
                        "in": "query"
                    },
                    {
                        "name": "categoria",
                        "description": "Filtra los productos por categoría.",
                        "schema": {
                            "type": "string"
                        },
                        "in": "query"
                    },
                    {
                        "name": "precioDesde",
                        "description": "Filtra productos con precio mayor o igual a este valor.",
                        "schema": {
                            "type": "number"
                        },
                        "in": "query"
                    },
                    {
                        "name": "precioHasta",
                        "description": "Filtra productos con precio menor o igual a este valor.",
                        "schema": {
                            "type": "number"
                        },
                        "in": "query"
                    },
                    {
                        "name": "orden",
                        "description": "Orden de los resultados.",
                        "schema": {
                            "enum": [
                                "masVendidos",
                                "precioAscendente",
                                "precioDescendente"
                            ],
                            "type": "string"
                        },
                        "in": "query"
                    }
                ],
                "responses": {
                    "200": {
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "type": "object",
                                        "properties": {
                                            "vendedor": {
                                                "type": "string"
                                            },
                                            "titulo": {
                                                "type": "string"
                                            },
                                            "descripcion": {
                                                "type": "string"
                                            },
                                            "categorias": {
                                                "type": "array",
                                                "items": {
                                                    "type": "string"
                                                }
                                            },
                                            "precio": {
                                                "type": "number"
                                            },
                                            "moneda": {
                                                "type": "string"
                                            },
                                            "stock": {
                                                "type": "integer"
                                            },
                                            "cantidadVendida": {
                                                "type": "integer"
                                            },
                                            "fotos": {
                                                "type": "array",
                                                "items": {
                                                    "type": "string"
                                                }
                                            },
                                            "activo": {
                                                "type": "boolean"
                                            }
                                        }
                                    }
                                },
                                "examples": {
                                    "example": {
                                        "summary": "Respuesta exitosa",
                                        "value": [
                                            {
                                                "vendedor": "Juan Pérez",
                                                "titulo": "Remera Azul",
                                                "descripcion": "Remera de algodón azul con estampado",
                                                "categorias": [
                                                    "Ropa",
                                                    "Hombre"
                                                ],
                                                "precio": 4500,
                                                "moneda": "ARS",
                                                "stock": 15,
                                                "cantidadVendida": 120,
                                                "fotos": [
                                                    "https://ejemplo.com/foto1.jpg"
                                                ],
                                                "activo": true
                                            }
                                        ]
                                    }
                                }
                            }
                        },
                        "description": "Lista de productos del vendedor."
                    },
                    "404": {
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "error": {
                                            "type": "string"
                                        }
                                    }
                                },
                                "example": {
                                    "error": "No se encontraron productos para el vendedor indicado."
                                }
                            }
                        },
                        "description": "No se encontró el vendedor o no existen productos asociados."
                    },
                    "500": {
                        "$ref": "#/components/responses/Internal-Server-ERROR"
                    }
                },
                "summary": "Obtiene los productos de un vendedor",
                "description": "Retorna los productos de un vendedor filtrados y ordenados. Si no se especifican parámetros de consulta, devuelve todos los productos del vendedor."
            }
        }
    },
    "components": {
        "schemas": {
            "Pedido": {
                "title": "Root Type for Pedido",
                "description": "Un pedio esta conformado por:\n    Un comprador\n    Un conjunto de Items\n    Una moneda\n    Una direccion",
                "type": "object",
                "properties": {
                    "compradorId": {
                        "description": "Id del comprador",
                        "type": "string"
                    },
                    "items": {
                        "description": "Items que conforman el pedido\n\n\n    [\n        {\n          producto;\n          cantidad;\n        }\n    ]",
                        "type": "array",
                        "items": {
                            "type": "object",
                            "properties": {
                                "productoId": {
                                    "type": "string"
                                },
                                "cantidad": {
                                    "format": "int32",
                                    "type": "integer"
                                }
                            }
                        }
                    },
                    "moneda": {
                        "description": "Moneda en la que se realizada el pago. Puediendo ser una de las siguientes\n\n    Peso: \"PESO_ARG\",\n    Dolar: \"DOLAR_USA\",\n    Real: \"REAL\"",
                        "type": "string"
                    },
                    "direccionEntrega": {
                        "description": "Direccion de entrega del pedido:\n\n    calle, altura, piso, departamento, ciudad, codPostal, provincia, pais, lat, lon",
                        "type": "array",
                        "items": {},
                        "properties": {
                            "calle": {
                                "type": "string"
                            },
                            "altura": {
                                "type": "string"
                            },
                            "piso": {
                                "type": "string"
                            },
                            "departamento": {
                                "type": "string"
                            },
                            "ciudad": {
                                "type": "string"
                            },
                            "codPostal": {
                                "type": "string"
                            },
                            "provincia": {
                                "type": "string"
                            },
                            "pais": {
                                "type": "string"
                            },
                            "lat": {
                                "format": "double",
                                "type": "number"
                            },
                            "lon": {
                                "format": "double",
                                "type": "number"
                            }
                        }
                    },
                    "estado": {
                        "description": "El estado actual del pedido. Puediendo ser\n\n    Pendiente: \"PENDIENTE\",\n    Confirmado: \"CONFIRMADO\",\n    EnPreparacion: \"EN_PREP\",\n    Enviado: \"ENVIADO\",\n    Entregado: \"ENTREGADO\",\n    Cancelado: \"CANCELADO\""
                    },
                    "historialEstados": {
                        "description": "El historial de estados que tuvo el pedido\n\n      estado;\n      pedido;\n      usuario;\n      motivo;\n      fecha;"
                    }
                },
                "example": {
                    "comprador": "68c9ab77b47a72e5d35c936b",
                    "items": [
                        {
                            "producto": {
                                "_id": "68c9b5b203365f3e368b0770",
                                "descripcion": "de buena calidad",
                                "categorias": [
                                    "comida"
                                ],
                                "precio": 232,
                                "moneda": "PESO_ARG",
                                "stock": 6,
                                "fotos": [
                                    "tuki"
                                ],
                                "activo": true,
                                "titulo": "LECHUGAA",
                                "vendedor": "68c9b787e22ba22fcb9d4e8a"
                            },
                            "cantidad": 1,
                            "precioUnitario": 232,
                            "_id": "68e5870e678270056223ba2f"
                        }
                    ],
